html {
    font-size: 100px;
    /*min-width: 1400px;*/
    height: 100%;
    overflow-x: hidden;
}

body {
    font-size: 12px;
    font-family: "微软雅黑";
    /*min-width: 960px;*/
}

.head,.page-head {
    width: 100%;
    position: fixed;
    height: 1rem;
    z-index: 100;
}

.su {
    width: 0.01rem;
    height: 0.15rem;
    background: #e3e3e3;
    float: left;
    margin-left: 0.29rem;
    margin-top: 0.45rem;
}

.head-hua {
    background: #fff;
