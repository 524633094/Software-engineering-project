}
.who-ltp ul li:hover{
    animation: rotation1 0.5s linear forwards;
    -moz-animation: rotation1 0.5s linear forwards;
    -webkit-animation: rotation1 0.5s linear forwards;
    -o-animation: rotation1 0.5s linear forwards;
}
@-webkit-keyframes rotation1 {
    from {
      background: #fff;
    }
    to {
      background: #f2f2f2;
    }
}
@-webkit-keyframes rotation {
    from {
      transform: scale(1); 
    }
    to {
      transform: scale(0.95);  
    }
}

