}

.logo {
    float: left;
    height: 1rem;
}

.logo img {
    padding: 0.3rem;
    width: 2rem;
    margin-top: -30px;
    zoom:  -1;
}

.nav,.page-nav {
    float: left;
    margin-left: 4rem;
    width: 8rem;
}
.page-head{
    background: #fff;
    box-shadow: 0 2px 6px rgba(0, 0, 0, .1)
}


.nav ul,.page-nav ul {
    width: 10rem;
    text-align: center;
    margin: 0 auto;
}

.nav ul li ,.page-nav ul li{
    float: left;
    font-size: 0.2rem;
    width: 1.4rem;
    line-height: 1rem;
    cursor: pointer;
    color: #333333;
}

.nav ul li a,.page-nav ul li a {
    color: #333333;
    width: 0.7rem;
    height: 100%;
    display: block;
    float: left;
}
.page-nav ul li:nth-child(2) a{
    color:#0b96d1;
    font-weight: bold;
}
.nav ul li .nav-active,.page-nav ul li .nav-active {
    padding-bottom: 0.1rem;
    color: #0b96d1;
    font-weight: bold;
}

.phone {
    float: right;
    height: 1rem;
    line-height: 1rem;
    padding-right: 0.5rem;
    font-size: 0.2rem;
    
    /*font-weight: bold;*/
}

.phone img {
    vertical-align: middle;
    width: 0.2rem;
}

#service,
#case,
#about,
#contact {
    position: relative;
    width: 100%;
}

#about {
    margin-top: 1rem;
}

.carousel-indicators .active {
    width: 10%;
    height: 4px;
    margin: 0;
    border: none;
    background-color: #0b96d1;
}

.carousel-indicators li {
    display: inline-block;
    width: 10%;
    height: 2px;
    margin: 1px;
    border: none;
    text-indent: -999px;
    cursor: pointer;
    background: #e1e1e1;
}

.carousel-indicators {
    bottom: 0.6rem;
}

#service ul li {
    position: relative;
    float: left;
    width: 20%;
}

.service-img {
    width: 100%;
    display: block;
}

.service1,
.service2,
.service3,
.service4,
.service5 {
    width: 100%;
    text-align: center;
    height: 1rem;
    line-height: 1rem;
}

.service1 {
    background: #d6b03d;
}

.service2 {
    background: #0b96d1;
}

.service3 {
    background: #dc4a4a;
}

.service4 {
    background: #0b96d1;
}
.service5 {
    background: #3bae76;
}

.service-span {
    font-size: 0.24rem;
    font-weight: bold;
    color: white;
    position: relative;
}

.service-icon {
    width: 0.5rem;
    vertical-align: middle;
}

.service-content {
    font-size: 0.15rem;
    background: #eeeeee;
    text-align: center;
    padding-bottom: 0.4rem;
    padding-top: 0.2rem;
    position: relative;
}

.service-content span {
    display: block;
    padding-top: 0.2rem;
    color: #666666;
}

.shadow {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 0.5rem;
}

.port-1 {
    width: 100%;
    position: relative;
    overflow: hidden;
    text-align: center;
}

.text-desc {
    position: absolute;
    left: 0;
    top: 0;
    background-color: black;
    height: 100%;
    opacity: 0;
    filter: Alpha(opacity=0);
    width: 100%;
}

.port-1 .text-desc {
    opacity: 0.7;
    filter: Alpha(opacity=70);
    top: -100%;
    transition: 0.5s;
    color: #000;
}

.text-desc span {
   font-size: 0.2rem;
   color: white;
   position: absolute;
   width: 100%;
   top: 50%;
   left: 50%;
   transform: translate(-50%,-50%);
}
.text-desc-sb{
   right: 15%!important; 
}

.port-1 img {
    width: 100%;
    transition: 0.5s;
    display: block;
}

.port-1:hover img {
    transform: scale(1.2);
}

.port-1.effect-1:hover .text-desc {
    top: 0;
}

.port-1.effect-2 .text-desc {
    top: auto;
    bottom: -100%;
}

.port-1.effect-2:hover .text-desc {
    bottom: 0;
}





