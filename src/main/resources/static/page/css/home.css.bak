#slide {
    margin: 0 auto;
    width: 7.5rem;
    height: 6rem;
    position: relative;
    margin-top: 0.3rem;
}

#slide li {
    float: left;
    position: absolute;
    width: 0.76rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    align-items: flex-start;
    -webkit-box-align: flex-start;
    -webkit-align-items: flex-start;
    background: #fff;
    overflow: hidden;
    box-shadow: 0 0 0.8rem #f1f1f1;
}

.arrow .prev,
.arrow .next {
    position: absolute;
    width: 0.64rem;
    top: 38%;
    z-index: 9;
    font-family: 'simsun';
    font-size: 0.96rem;
    font-weight: 700;
    font: 700 0.96rem;
    opacity: 0.3;
    color: black;
    cursor: pointer;
}

.arrow .prev {
    left: -4.5rem;
    top: 3rem;
    z-index: 9;
}

.arrow .next {
    right: -4.5rem;
    top: 3rem;
    z-index: 9;
}

.arrow .prev:hover,
.arrow .next:hover {
    color: #00a0e9;
    opacity: .7;
    z-index: 9;
}

